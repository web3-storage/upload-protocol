{
  "name": "@web3-storage/upload",
  "version": "0.0.0-dev",
  "main": "src/index.js",
  "types": "dist/src/index.d.ts",
  "typedocMain": "dist/src/index.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "url": "web3-storage/upload-protocol",
    "directory": "packages/upload"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/src/*"
      ],
      "types": [
        "src/types.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint '**/*.{js,ts}' && prettier --check '**/*.{js,ts,md,yml,json}' --ignore-path ../../.gitignore && tsc",
    "docs": "pnpm run lint && typedoc src/ucan-storage.js src/keypair.js --out docs",
    "test": "DOTENV_CONFIG_PATH=../../.env mocha -r dotenv/config -r test/mocks/hooks.js",
    "test-browser": "pw-test test",
    "testw": "watch 'pnpm test' src test --interval 1",
    "server": "node test/mocks/server.js"
  },
  "files": [
    "src",
    "dist/src/*.d.ts",
    "dist/src/*.d.ts.map"
  ],
  "dependencies": {
    "@ipld/car": "^4.0.0",
    "@ipld/unixfs": "^1.0.3-dev",
    "@noble/hashes": "^1.0.0",
    "@web-std/blob": "^3.0.4",
    "@web-std/fetch": "^4.0.0",
    "@web-storage/signer": "workspace:*",
    "conf": "^10.1.1",
    "multiformats": "^9.6.4",
    "ora": "^6.1.0",
    "sade": "^1.8.1",
    "ucan-storage": "^1.1.3"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/polka": "^0.5.4",
    "assert": "^2.0.0",
    "debug": "^4.3.4",
    "delay": "^5.0.0",
    "dotenv": "^16.0.0",
    "execa": "^6.1.0",
    "mocha": "^9.2.2",
    "playwright-test": "^7.3.0",
    "polka": "^0.5.2",
    "watch": "^1.0.2"
  },
  "eslintConfig": {
    "extends": [
      "../../node_modules/hd-scripts/eslint/index.js"
    ],
    "ignorePatterns": [
      "pw-test.config.cjs"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "mocha": true
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "docs"
  ],
  "engines": {
    "node": ">= 14.0.0"
  }
}
